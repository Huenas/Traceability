@model EquipementViewModel
    <head>
        <link rel="stylesheet" href="css/assets/css/fontawesome.css">
        <link rel="stylesheet" href="css/assets/css/templatemo-plot-listing.css">
        <link rel="stylesheet" href="css/assets/css/animated.css">
        <link rel="stylesheet" href="css/assets/css/owl.css">
    </head>
<div class="page-header">
    <h1>Product traceability Equipement detail</h1>
</div>
<div class="row">
    <div class="col-md-4">
        <div class="panel panel-default">            
            <table class="table">
                <thead>
                    <tr>
                        <th class="text-nowrap" scope="col"> Nom du Controleur  </th>                   
                    </tr>
                </thead>
                <tbody>
                    @{ foreach (var item in Model.listEquipement)
                        {
                            <tr>                                                                                                            
                                <td>@item.NomEquipement</td>                               
                            </tr>
                        }
                    }
                </tbody>
            </table>
        </div>
    </div>
</div>