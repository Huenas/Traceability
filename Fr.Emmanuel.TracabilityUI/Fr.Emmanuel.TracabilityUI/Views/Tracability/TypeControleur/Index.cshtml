@model TypeControleurViewModel


    <head>
        <link rel="stylesheet" href="css/assets/css/fontawesome.css">
        <link rel="stylesheet" href="css/assets/css/templatemo-plot-listing.css">
        <link rel="stylesheet" href="css/assets/css/animated.css">
        <link rel="stylesheet" href="css/assets/css/owl.css">
    </head>



<div class="page-header">
    <h1>Product traceability Types of Controleurs detail</h1>
</div>
<div class="row">
    <div class="col-md-4">
        <div class="panel panel-default">
            
            <table class="table">
                <thead>
                    <tr>
                        <th class="text-nowrap" scope="col"> Id Type Controleurs  </th>
                        <th class="text-nowrap" scope="col"> Tous les libelles</th>
                    </tr>
                </thead>
                <tbody>
                    @{ foreach (var item in Model.listTypeControleur)
                        {
                        <tr>
                            <td>@item.IdTypeControleur</td>                   
                            <td>@item.LibelleType</td>                                                      
                        </tr> 
                            }
                        }
                </tbody>
            </table>
        </div>
    </div>   
</div>